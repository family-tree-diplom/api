{
    "require": {
        "opencck/server": "v1.0.0",
        "amphp/http-server-session": "v3.0.0",
        "amphp/redis": "v2.0.0",
        "amphp/parallel": "v2.1.0",
        "amphp/file": "v3.0.0",
        "amphp/http-tunnel": "v2.0.0-beta.2",
        "cycle/annotated": "^4",
        "doctrine/annotations": "^2",
        "vlucas/phpdotenv": "^5.5",
        "doctrine/collections": "^2.1",
        "react/mysql": "^0.5.7",
        "revolt/event-loop-adapter-react": "^1.1",
        "drift/dbal": "^0.1.5",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "phpmailer/phpmailer": "^v6.10.0",
        "nyholm/psr7": "^1.8@dev",
        "amphp/http-client": "v5.0.1",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "amphp/phpunit-util": "^3"
    },
    "autoload": {
        "files": [
            "packages/opencck/core/src/functions.php"
        ],
        "psr-4": {
            "OpenCCK\\": [
                "src/",
                "packages/opencck/core/src/",
                "packages/opencck/db/src/",
                "packages/opencck/http-server/src/",
                "packages/opencck/http-server-auth/src/",
                "packages/opencck/mail/src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenCCK\\": [
                "test/",
                "packages/opencck/core/test/"
            ]
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
